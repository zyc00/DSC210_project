<?xml version="1.0" ?>
<robot name="left_end_effector">
  <xacro:include filename="$(find rethink_ee_description)/urdf/electric_gripper/rethink_electric_gripper.xacro" />
  <xacro:rethink_electric_gripper side="left"
                                  l_finger="extended_narrow"
                                  l_finger_slot="2"
                                  l_finger_tip="paddle_tip"
                                  l_finger_grasp="inner"
                                  r_finger="extended_narrow"
                                  r_finger_slot="2"
                                  r_finger_tip="paddle_tip"
                                  r_finger_grasp="inner"/>
</robot>



<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="rethink_electric_gripper">
  <!-- <xacro:macro name="rethink_electric_gripper" params="side r_finger r_finger_slot r_finger_tip r_finger_grasp l_finger l_finger_slot l_finger_tip l_finger_grasp"> -->
    <!-- <xacro:property name="gripper_side" value="left" scope="local"/> -->
    <!-- Base of end effector -->
    <link name="left_gripper_base">
      <visual>
        <origin rpy="-1.57 3.14 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://rethink_ee_description/meshes/electric_gripper/electric_gripper_base.DAE"/>
        </geometry>
      </visual>
      <collision>
        <origin rpy="-1.57 3.14 0" xyz="0.0 0.0 0"/>
        <geometry>
          <cylinder length="0.1" radius="0.029"/>
        </geometry>
        <material name="darkred">
          <color rgba=".5 .1 .1 1"/>
        </material>
      </collision>
      <inertial>
        <origin rpy="-1.57 3.14 0" xyz="0.0 0.0 0.0"/>
        <mass value="0.3"/>
        <inertia ixx="2e-08" ixy="0" ixz="0" iyy="3e-08" iyz="0" izz="2e-08"/>
      </inertial>
    </link>

    <!-- Left finger link -->
    <xacro:property name="finger_length" value="0.0" scope="global"/>
    <xacro:include filename="$(find rethink_ee_description)/urdf/electric_gripper/fingers/extended_narrow.xacro" />
    <xacro:finger_xacro gripper_side="left" finger_side="l" reflect="1" finger_tip="paddle_tip" finger_grasp="inner"/>
    <xacro:property name="tip_finger_length" value="${finger_length}" scope="local"/>

    <!-- Right finger link -->
    <xacro:include filename="$(find rethink_ee_description)/urdf/electric_gripper/fingers/extended_narrow.xacro" />
    <xacro:finger_xacro gripper_side="${gripper_side}" finger_side="r" reflect="-1" finger_tip="paddle_tip" finger_grasp="inner"/>
    <xacro:if value="${finger_length > tip_finger_length}">
        <xacro:property name="tip_finger_length" value="${finger_length}" scope="local"/>
    </xacro:if>

    <!-- Electric Gripper Base Joint -->
    <joint name="$left_gripper_base" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 0.025"/>
      <parent link="left_hand"/>
      <child link="left_gripper_base"/>
    </joint>
    <!-- Electric Gripper Tip joint -->
    <joint name="left_endpoint" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 ${tip_finger_length+0.02}"/>
      <parent link="left_gripper_base"/>
      <child link="left_gripper"/>
    </joint>
    <!-- Electric Gripper Tip link -->
    <link name="${side}_gripper">
      <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.0001"/>
        <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0.0"/>
      </inertial>
    </link>

    <!-- Left finger -->
    <xacro:property name="l_finger_offset" value="-0.0015" scope="local"/>
    <joint name="${gripper_side}_gripper_l_finger_joint" type="prismatic">
      <origin rpy="0 0 0" xyz="0.0 ${l_finger_offset} 0.02"/>
      <axis xyz="0 1 0"/>
      <parent link="left_gripper_base"/>
      <child link="${gripper_side}_gripper_l_finger"/>
      <limit effort="20.0" lower="0.0" upper="0.020833" velocity="5.0"/>
      <dynamics damping="0.7" friction="0.0"/>
    </joint>

    <!-- Right finger -->
    <xacro:property name="r_finger_offset" value="0.0015" scope="local"/>
    <joint name="${gripper_side}_gripper_r_finger_joint" type="prismatic">
      <origin rpy="0 0 0" xyz="0.0 ${r_finger_offset} 0.02"/>
      <axis xyz="0 1 0"/>
      <parent link="left_gripper_base"/>
      <child link="${gripper_side}_gripper_r_finger"/>
      <limit effort="20.0" lower="-0.020833" upper="0.0" velocity="5.0"/>
      <mimic joint="${gripper_side}_gripper_l_finger_joint" multiplier="-1.0"/>
      <dynamics damping="0.7" friction="0.0"/>
    </joint>

    <transmission name="gripper_left1">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${gripper_side}_gripper_r_finger_joint">
        <hardwareInterface>EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="gripper_${gripper_side}1_motor1">
        <hardwareInterface>EffortJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
    <transmission name="gripper_left2">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${gripper_side}_gripper_l_finger_joint">
        <hardwareInterface>EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="gripper_${gripper_side}1_motor2">
        <hardwareInterface>EffortJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
    <gazebo reference="${gripper_side}_gripper_r_finger_joint">
      <implicitSpringDamper>1</implicitSpringDamper>
    </gazebo>
    <gazebo reference="${gripper_side}_gripper_l_finger_joint">
      <implicitSpringDamper>1</implicitSpringDamper>
    </gazebo>

  </xacro:macro>
</robot>
